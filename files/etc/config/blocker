config blocker 'global'
    option enabled '1'
    option block_dns '1'
    option block_ip '1'
    option block_app '1'
    list blocklist_urls 'https://raw.githubusercontent.com/StevenBlack/hosts/master/hosts'
    list blocklist_urls 'https://malware-filter.gitlab.io/malware-filter/urlhaus-filter.txt'
